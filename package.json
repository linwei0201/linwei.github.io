{
  "name": "Big-Big-World",
  "version": "0.0.1",
  "description": "a react application powerd by zola",
  "scripts": {
    "start": "ENVIRONMENT=development redskull start --dashboard false",
    "test": "ENVIRONMENT=testing redskull build",
    "build": "ENVIRONMENT=production redskull build --publicPath https://linwei0201.github.io/dist/ && cp -rf ./dist/index.html ./index.html",
    "build2": "ENVIRONMENT=production redskull build --publicPath http://missweiweian.com/dist/ && cp -rf ./dist/index.html ./index.html",
    "local": "ENVIRONMENT=production redskull build --publicPath http://127.0.0.1:8080/dist/ && cp -rf ./dist/index.html ./index.html"
  },
  "license": "ISC",
  "dependencies": {
    "highlight.js": "^9.12.0",
    "html-loader": "^0.4.5",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-router-dom": "^4.1.1",
    "showdown": "^1.7.1",
    "summarize-markdown": "^0.3.1",
    "watch": "^1.0.2"
  },
  "redskull": {
    "port": 6003,
    "src": "./src",
    "entry": "index.js",
    "dist": "./dist",
    "openBrowser": false,
    "devConfig": "./webpack/config.dev.js",
    "prodConfig": "./webpack/config.prod.js",
    "favico": "./src/favicon.ico"
  }
}
